#include <stdio.h>
int main(void) {
	int t;
	scanf("%d",&t);
	int n,num;
	for(int i=0;i<t;i++)
	{
	    scanf("%d",&n);
	    if(n<=100)
	    num=n;
	    else if(n>100 && n<=1000)
	    num=n-25;
	    else if(n>1000 && n<=5000)
	    num=n-100;
	    else if(n>5000)
	    num=n-500;
	    printf("%d\n",num);
	}

}

