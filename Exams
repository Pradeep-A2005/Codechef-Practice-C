#include <stdio.h>
int main(void) {
    int t;
    scanf("%d", &t);
    int x, y, z;
    for(int i = 0; i < t; i++) {
        scanf("%d %d %d", &x, &y, &z);
        double per = (double)x * y;
        double per1 = (double)z * 100;
        double percentage = per1 / per;
        (percentage > 50) ? printf("yes\n") : printf("no\n");
    }
    return 0;
}
