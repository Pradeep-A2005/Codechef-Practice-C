#include <stdio.h>
int main(void) {
	int n;
	scanf("%d",&n);
	int a[n];
	for(int i=0;i<n;i++){
	    scanf("%d",&a[i]);
	}
	int odd=0;
	int even=0;
	for(int i=0;i<n;i++){
	    if(a[i]%2==0)
	    even++;
	    else
	    odd++;
	}
	if(even>odd)
	printf("READY FOR BATTLE");
	else
	printf("NOT READY");
}
