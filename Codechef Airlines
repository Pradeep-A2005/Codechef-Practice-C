#include <stdio.h>
int main(void) 
{
	int t,x,y,z;
	scanf("%d",&t);
	for(int i=0;i<t;i++)
	{
	    scanf("%d%d%d",&x,&y,&z);
	    int x1=x*10;
	    if(x1<=y)
	        printf("%d\n",x1*z);
	        
	    else if(x1>=y)
	        printf("%d\n",y*z);
	        
	    else
	        printf("%d\n",x1*y);
	}
}

