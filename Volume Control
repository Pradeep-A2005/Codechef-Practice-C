#include <stdio.h>
int main(void) {
	int t,x,y;
	scanf("%d",&t);
	for(int i=0;i<t;i++){
	    int c=0;
	    scanf("%d %d",&x,&y);
	    if(x<y){
	    for(int j=x;j<y;j++){
	        c++;
	        }
	    }
	    else{
	        for(int j=y;j<x;j++){
	        c++;
	    }
	    }
	    printf("%d\n",c);
	}
}

